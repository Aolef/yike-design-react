  },
  "clean-package": "./clean-package.config.json",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.22.6"
  },
  "devDependencies": {
    "@types/webpack-env": "^1.18.2",
    "clean-package": "^2.2.0"
  }
